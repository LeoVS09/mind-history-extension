{
  "manifest_version": 2,
  "name": "Mind History",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png",
    "34": "icon-34.png"
  },
  "web_accessible_resources": [
    "content.styles.css",
    "icon-128.png",
    "icon-34.png"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
    "history",
    "webNavigation",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ]
}